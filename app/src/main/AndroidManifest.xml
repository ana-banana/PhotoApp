<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.photo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivityPhotos" android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
        <activity android:name=".UploadPhotoActivity"/>
        <activity android:name=".MainActivityGalleryMode" android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
        <activity android:name=".GalleryViews"/>
        <activity android:name=".MainActivityRatingMode" android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>
        <activity android:name=".OnePhotoActivity"/>

    </application>

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    
    

</manifest>
